{
  "name": "hugo-starter-pluto",
  "version": "1.0.0",
  "description": "A Hugo starter template for new projects powered by Pluto",
  "author": "The Pluto Authors",
  "license": "MIT",
  "homepage": "https://pluto.land",
  "repository": "plutoland/hugo-starter-pluto",
  "keywords": [
    "pluto",
    "plutoland",
    "design-system",
    "starter",
    "template",
    "hugo",
    "hugo-starter"
  ],
  "scripts": {
    "clean": "rimraf public",
    "prebuild": "npm run clean",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:hugo": "hugo -s site -d ../public -v",
    "serve": "live-server ./public --no-browser --port=3000",
    "develop": "concurrently \"npm run develop:webpack\" \"npm run develop:hugo\"",
    "develop:webpack": "webpack-dev-server --config webpack.dev.js",
    "develop:hugo": "hugo -s site -d ../public -vw",
    "start": "npm run develop"
  },
  "dependencies": {
    "@plutoland/css": "^1.0.0-canary.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "assets-webpack-plugin": "^5.1.2",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "hugo-bin": "^0.62.3",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^7.0.35",
    "postcss-loader": "^3.0.0",
    "postcss-node-sass": "^2.1.8",
    "postcss-scss": "^2.1.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^3.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
